    "plistCommands": [
        {
            "command": "com.apple.finder ShowHardDrivesOnDesktop -bool true",
            "description": "Show Hard Drives on Desktop"
        },
        {
            "command": "com.apple.finder ShowExternalHardDrivesOnDesktop -bool true",
            "description": "Show External Hard Drives on Desktop"
        },
        {
            "command": "com.apple.finder ShowRemovableMediaOnDesktop -bool true",
            "description": "Show Removable Media on Desktop"
        },
        {
            "command": "com.apple.finder ShowMountedServersOnDesktop -bool true",
            "description": "Show Mounted Servers on Desktop"
        },
        {
            "command": "com.apple.finder ShowStatusBar -bool true",
            "description": "Show Status Bar in Finder"
        },
        {
            "command": "com.apple.finder FXPreferredViewStyle clmv",
            "description": "Set Finder view style to Column View"
        },
        {
            "command": "NSGlobalDomain AppleInterfaceStyle Dark",
            "description": "Set Dark Mode"
        },
        {
            "command": "NSGlobalDomain NSNavPanelExpandedStateForSaveMode -bool true",
            "description": "Expand save panel by default"
        },
        {
            "command": "com.apple.LaunchServices LSQuarantine -bool false",
            "description": "Disable Quarantine"
        },
        {
            "command": "com.apple.loginwindow TALLogoutSavesState -bool false",
            "description": "Disable Reopen Windows at Login"
        },
        {
            "command": "com.apple.loginwindow LoginwindowLaunchesRelaunchApps -bool false",
            "description": "Disable Relaunch Apps at Login"
        },
        {
            "command": "com.apple.desktopservices DSDontWriteNetworkStores -bool true",
            "description": "Disable creating .DS_Store on Network Volumes"
        },
        {
            "command": "com.apple.TimeMachine DoNotOfferNewDisksForBackup -bool true",
            "description": "Disable Time Machine prompt for new disks"
        },
        {
            "command": "NSGlobalDomain PMPrintingExpandedStateForPrint -bool true",
            "description": "Expand print panel by default"
        },
        {
            "command": "com.apple.WindowManager EnableStandardClickToShowDesktop -bool false",
            "description": "Disable Show Desktop on standard click"
        },
        {
            "command": "com.apple.LaunchServices/com.apple.launchservices.secure LSHandlers -array-add '{LSHandlerURLScheme=\"http\";LSHandlerRoleAll=\"com.google.chrome\";}'",
            "description": "Set Chrome as default browser for http"
        },
        {
            "command": "com.apple.LaunchServices/com.apple.launchservices.secure LSHandlers -array-add '{LSHandlerURLScheme=\"https\";LSHandlerRoleAll=\"com.google.chrome\";}'",
            "description": "Set Chrome as default browser for https"
        },
        {
            "command": "com.apple.LaunchServices/com.apple.launchservices.secure LSHandlers -array-add '{LSHandlerContentType=public.url;LSHandlerRoleAll=com.google.chrome;}'",
            "description": "Set Chrome as default browser for public.url"
        },
        {
            "command": "com.apple.LaunchServices/com.apple.launchservices.secure LSHandlers -array-add '{LSHandlerURLScheme=\"mailto\";LSHandlerRoleAll=\"com.microsoft.outlook\";}'",
            "description": "Set Outlook as default mail client for mailto"
        },
        {
            "command": "com.apple.LaunchServices/com.apple.launchservices.secure LSHandlers -array-add '{LSHandlerContentType=com.adobe.pdf;LSHandlerRoleAll=com.adobe.reader;}'",
            "description": "Set Adobe Reader as default PDF viewer"
        },
        {
            "command": "com.apple.LaunchServices/com.apple.launchservices.secure LSHandlers -array-add '{LSHandlerURLScheme=reader2021.oauth2;LSHandlerRoleAll=com.adobe.reader;}'",
            "description": "Set Adobe Reader as default PDF viewer"
        },
        {
            "command": "com.apple.LaunchServices/com.apple.launchservices.secure LSHandlers -array-add '{LSHandlerURLScheme=acrobat2018;LSHandlerRoleAll=com.adobe.reader;}'",
            "description": "Set Adobe Reader as default PDF viewer"
        },
        {
            "command": "com.adobe.Reader.plist \"DC:WTAOnBoarding:WTAOnBoardingState:1\" -int 2",
            "description": "Set Adobe Reader as Onboard Complete"
        },
        {
            "command": "com.adobe.Reader.plist \"DC:WTAIsOnBoardingExpCompleted:1\" -bool 1",
            "description": "Set Adobe Reader as Onboard Exp Completed"
        },
        {
            "command": "com.adobe.Reader.plist \"DC:ShowSplashScreen\" -bool 0",
            "description": "Disable Adobe Reader Splash Screen"
        },
        {
            "command": "com.adobe.Reader.plist \"DC:EULAAccepted\" -bool 1",
            "description": "Accept Adobe Reader EULA"
        },
        {
            "command": "com.centrastage.tray.plist \"NSStatusItem Visible Item-0\" -bool false",
            "description": "Hide DattoRMM menu bar icon"
        },
        {
            "command": "com.centrastage.tray.plist \"NSStatusItem Preferred Position Item-0\" -float 0.0",
            "description": "Hide DattoRMM menu bar icon"
        },
        {
            "command": "nl.root3.support \"NSStatusItem Preferred Position Item-0\" -float 200",
            "description": "Set SupportApp icon position"
        },
        {
            "command": "com.apple.Spotlight \"NSStatusItem Preferred Position Item-0\" -float 150",
            "description": "Set Spotlight icon position"
        },
        {
            "command": "com.apple.controlcenter \"NSStatusItem Preferred Position BentoBox\" -float 100",
            "description": "Set BentoBox icon position"
        },
        {
            "command": "com.apple.controlcenter \"NSStatusItem Preferred Position UserSwitcher\" -float 250",
            "description": "Set UserSwitcher icon position"
        },
        {
            "command": "com.screenconnect.client.access \"NSStatusItem Visible Item-0\" -bool false",
            "description": "Set ScreenConnect icon visibility"
        },
        {
            "command": "com.screenconnect.client.access \"NSStatusItem Preferred Position Item-0\" -float -1",
            "description": "Set ScreenConnect icon visibility"
        }
    ],
    "osascriptCommands": [
        "tell app \"System Events\" to tell appearance preferences to set dark mode to true",
        "tell application \"System Events\" to set picture of every desktop to \"/Users/Shared/wallpaper.png\""
    ]
